<div>
   <%= render "banner" %>
</div>
<div class="container list-cards">
  <div class="row">
    <h1 style="color:white;">My Lists > </h1>
  </div>
  <div class="row">
    <% @lists.each do |list| %>
      <div id="list-card" class="col-6 my-3">
        <%= link_to list_path(list) do %>
          <div class="list-card" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(https://raw.githubusercontent.com/lewagon/fullstack-images/master/uikit/breakfast.jpg)">
             <h1><%= list.name %></h1>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>
  <div class="row">
  <div class="owl-carousel owl-theme">
    <div class="item"><h4>1</h4></div>
    <div class="item"><h4>2</h4></div>
    <div class="item"><h4>3</h4></div>
    <div class="item"><h4>4</h4></div>
    <div class="item"><h4>5</h4></div>
    <div class="item"><h4>6</h4></div>
    <div class="item"><h4>7</h4></div>
    <div class="item"><h4>8</h4></div>
    <div class="item"><h4>9</h4></div>
    <div class="item"><h4>10</h4></div>
    <div class="item"><h4>11</h4></div>
    <div class="item"><h4>12</h4></div>
  </div>
</div>
